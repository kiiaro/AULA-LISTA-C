Questão 25:
#include <stdio.h>

int main() {
    float fahren, celsius;
    
    printf("Digite a temperatura em Fahrenheit: ");
    scanf("%f", &fahren);
    
    celsius = (fahren - 32) * 5.0 / 9.0;
    
    printf("%1.2f Fahrenheit equivale a %1.2f Celsius\n", fahren, celsius);
    
    return 0;
}
